{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-empty-interface": false,
    "no-console": false
  },
  "rulesDirectory": []
}